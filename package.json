{
	"name": "better-meetings",
	"version": "1.0.0",
	"description": "Be nice. Share space. Have better meetings.",
	"scripts": {
		"start:app": "webpack-dev-server --config build-config/webpack/webpack.dev.ts",
		"start:api": "now -A now.dev.json dev",
		"start": "concurrently 'yarn start:app' 'yarn start:api'",
		"build": "webpack --config build-config/webpack/webpack.prod.ts",
		"now-build": "yarn build",
		"serve": "serve -s dist",
		"deploy": "now --target staging -b NODE_ENV=staging",
		"deploy:prod": "now --target production -b NODE_ENV=production",
		"test": "jest",
		"test:coverage": "yarn test --coverage",
		"lint:css": "stylelint './src/**/*.tsx'",
		"lint:ts": "eslint '**/*.ts?(x)'",
		"lint": "yarn lint:css && yarn lint:ts",
		"gen-gql-types": "yarn with-api-config ts-node scripts/gen-gql-types.ts",
		"update-hasura-queries": "yarn with-api-config ts-node scripts/update-hasura-queries.ts",
		"with-api-config": "NODE_APP_INSTANCE='api'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ecobee/better-meetings.git"
	},
	"author": "ecobee <https://github.com/ecobee>",
