{
	"name": "better-meetings",
	"version": "1.0.0",
	"description": "Be nice. Share space. Have better meetings.",
	"scripts": {
		"start:app": "yarn with-node-tsconfig webpack-dev-server --config build-config/webpack/webpack.dev.ts",
		"start:api": "now -A now.dev.json dev",
		"start": "concurrently 'yarn start:app' 'yarn start:api'",
		"build": "yarn with-node-tsconfig webpack --config build-config/webpack/webpack.prod.ts",
		"now-build": "yarn build",
		"serve": "serve -s dist",
		"deploy": "now --target staging",
		"test": "jest",
		"test:coverage": "yarn test --coverage",
		"lint:css": "stylelint './src/**/*.tsx'",
		"lint:ts": "tslint --project .",
		"lint:api": "cd api&& tslint --project .",
		"lint": "yarn lint:css && yarn lint:ts && yarn lint:api",
		"gen-gql-types": "yarn with-api-config ts-node scripts/gen-gql-types.ts",
		"with-node-tsconfig": "TS_NODE_PROJECT='tsconfig-for-node.json'",
		"with-api-config": "NODE_APP_INSTANCE='api' yarn with-node-tsconfig"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ecobee/better-meetings.git"
	},
	"author": "ecobee <https://github.com/ecobee>",
