{
	"extends": ["tslint-config-airbnb", "tslint-config-prettier"],
	"rules": {
		"arrow-parens": [true, "ban-single-arg-parens"],
		"no-use-before-declare": false,
		"quotemark": [true, "single", "jsx-double"],
		"interface-name": false,
		"import-name": false,
		"variable-name": [
			true,
			"check-format",
			"allow-pascal-case",
			"allow-leading-underscore"
		]
	}
}
